    </material>
  </visual>

<sensor type="ray" name="lidar">
  <ray>
    <scan>
      <horizontal> <!-- Ang resolution 0.45 deg = 0,00785 rad--> 
        <samples>280</samples> <!-- samples = (max_angle - min_angle)/ang_resolution --> 
        <resolution>1</resolution> <!-- 1 for 2D Lidar --> 
        <min_angle>-1.099</min_angle> <!-- -63 deg -> -1.099 rad --> 
        <max_angle>1.099</max_angle> <!-- 63 deg -> 1.099 rad --> 
      </horizontal>
    </scan>
    <range> <!-- 0.2 - 12 [m] -->
      <min>0.2</min>
      <max>12.0</max>
      <resolution>0.01</resolution>
    </range>
  </ray>
</sensor>

</link>

<!-- Connection lidar with car -->
<joint name="lidar_joint" type="fixed">
  <parent>chassis</parent>
  <child>lidar_link</child>
  <pose>0 0 0 0 0 0</pose>
</joint>

<!-- Plugin for sensors -->
<plugin name='gz::sim::systems::Sensors' filename='gz-sim-sensors-system'>
  <render_engine>ogre2</render_engine>
  <start_enabled>true</start_enabled>
</plugin>




      
    </model>

    <light name='sun' type='directional'>
      <pose>0 0 10 0 0 0</pose>
      <cast_shadows>true</cast_shadows>
      <intensity>1</intensity>
      <direction>-0.5 0.1 -0.9</direction>
      <diffuse>1 1 1 1</diffuse>
      <specular>0.5 0.5 0.5 1</specular>
      <attenuation>
        <range>1000</range>
        <linear>0.01</linear>
        <constant>0.9</constant>
        <quadratic>0.001</quadratic>
      </attenuation>
      <spot>
        <inner_angle>0</inner_angle>
        <outer_angle>0</outer_angle>
        <falloff>0</falloff>
      </spot>
    </light>
  </world>
</sdf>
