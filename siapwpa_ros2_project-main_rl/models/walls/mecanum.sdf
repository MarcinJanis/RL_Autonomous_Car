  <pose>1.1 0 0.4 0 0 0</pose> <!-- --> 

  <inertial>
    <mass>0.1</mass>
    <inertia>
      <ixx>0.001</ixx>
      <iyy>0.001</iyy>
      <izz>0.001</izz>
    </inertia>
  </inertial>

  <visual name="lidar_visual">
